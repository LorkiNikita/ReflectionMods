          "Parent":  -1,
          "#":  6,
          "Total.Lines":  466,
          "Code.Type":  "Init",
          "Code":          
          [
              "nextattack = CurTurn()+Rnd(10,30);",
              "InitVars();",
              "updater=CT('Update.StatsVersion');",
              "function InitVars(){",
              "    ArrayClear(fem_name);",
              "    for(i = 1;i <= CT('Script.RefSpAgentDefence.NamesSpAgent.Count');i = i + 1){",
              "        ArrayAdd(fem_name,CT('Script.RefSpAgentDefence.NamesSpAgent.'+i));",
              "    }",
              "    for(i = 1;i <= CT('Script.RefSpAgentDefence.SkillsDiv.Count');i = i + 1){",
              "        UpdateSkillDivp(CT('Script.RefSpAgentDefence.SkillsDiv.'+i));",
              "    }",
              "    for(i = 1;i <= CT('Script.RefSpAgentDefence.SkillsWarr.Count');i = i + 1){",
              "        UpdateSkillWarr(CT('Script.RefSpAgentDefence.SkillsWarr.'+i));",
              "    }",
              "}",
              "function UpdateSkillDivp(str skill){",
              "    if(ArrayFind(skilldivpc,skill) == -1){",
              "        ArrayAdd(skilldivp,0,skill);",
              "        ArrayAdd(skilldivpc,skill);",
              "    }",
              "}",
              "",
              "function UpdateSkillWarr(str skill){",
              "    if(ArrayFind(skillwarrc,skill)== -1){",
              "        ArrayAdd(skillwarr,0,skill);",
              "        ArrayAdd(skillwarrc,skill);",
              "    }",
              "}",
              "",
              "function SpawnWarrior(dword planet){",
              "    dword ship = BuyWarrior(planet,2000);",
              "    if(skillwarr['BonHull']){",
              "        ItemSize(ShipItems(ship,0),Rnd(400,800)+max(3,GalaxyTechLevel())*10);",
              "    }",
              "    else {",
              "        ItemSize(ShipItems(ship,0),Rnd(700,1200)+max(3,GalaxyTechLevel())*20);",
              "    }",
              "",
              "    //Назначаем также и хп",
              "    HullHP(ship,ItemSize(ShipItems(ship,0)));",
              "    ShipType(ship,'Swat');",
              "    hullstat=ShipItems(ship,0);",
              "    //Делаем корпус",
              "    if(GetEquipmentStats(hullstat,5)<1)ShipSpecialBonuses(ship,bonSlotDroid,1);",
              "    if(GetEquipmentStats(hullstat,7)<1)ShipSpecialBonuses(ship,bonSlotDef,1);",
              "    ShipSpecialBonuses(ship,bonSlotWeapon,5);",
              "    ShipSpecialBonuses(ship,bonSlotArt,4);",
              "    if(skillwarr['Speed']){",
              "        ShipSpecialBonuses(ship,bonSpeed,400);",
              "        ShipSpecialBonuses(ship,bonDef,-2);",
              "        ShipSpecialBonuses(ship,bonDroid,-3);",
              "    }",
              "    else {",
              "        ShipSpecialBonuses(ship,bonSpeed,100);",
              "    }",
              "    if(skillwarr['BonSpHull']){",
              "        int hullbonus = FindBonusByCustomTag('SpAgentHull');",
              "        SpecialToEquipment(hullbonus, hullstat);",
              "    }",
              "    if(skillwarr['BufAttack']){",
              "        ShipSpecialBonuses(ship,bonWEnergy,15);",
              "        ShipSpecialBonuses(ship,bonWSplinter,15);",
              "        ShipSpecialBonuses(ship,bonWMissile,15);",
              "        ShipSpecialBonuses(ship,bonWRadius,50);",
              "        dword equip;",
              "        int rnd;",
              "        //Оснащаем артами",
              "        if(Rnd(1,5)>2){",
              "            rnd = Rnd(1,4);",
              "            if(rnd == 1){",
              "                equip=CreateArt(t_ArtefactHull,ShipOwner(ship));",
              "            }",
              "            else if(rnd == 2){",
              "                equip=CreateArt(t_ArtEnergyPulse,ShipOwner(ship));",
              "            }",
              "            else if(rnd == 3){",
              "                equip=CreateArt(t_ArtefactNano,ShipOwner(ship));",
              "            }",
              "            else {",
              "                equip=CreateArt(t_ArtSplinter,ShipOwner(ship));",
              "            }",
              "        }",
              "        AddItemToShip(ship,equip);",
              "        ItemIsInUse(equip,ship,1);",
              "    }",
              "    Chameleon(ship);",
              "    //Вешаем навыки",
              "    ShipSkill(ship,0,5);",
              "    ShipSkill(ship,1,5);",
              "    ShipSkill(ship,2,Rnd(3,6));",
              "    //Заставляем закупится корабль",
              "    for(i=0;i<4;i = i +1) {",
              "        ShipRefit(ship);",
              "    }",
              "    ShipJoin(WarriorsR,ship);",
              "}",
              "function SpawnAgent(dword planet) {",
              "    planetRace = PlanetRace(planet);",
              "    PlanetRace(planet,2);",
              "    dword ship = BuyRanger(planet,2000);",
              "    PlanetRace(planet,planetRace);",
              "    hullstat=ShipItems(ship,0);",
              "    ItemSize(ShipItems(ship,0),Rnd(250,750));",
              "    HullHP(ship,ItemSize(ShipItems(ship,0)));",
              "    Chameleon(ship,'Ship.AkrinFemale.J');",
              "    if(GetEquipmentStats(hullstat,5)<1)ShipSpecialBonuses(ship,bonSlotDroid,1);",
              "    if(GetEquipmentStats(hullstat,7)<1)ShipSpecialBonuses(ship,bonSlotDef,1);",
              "    ShipSpecialBonuses(ship,bonSlotWeapon,5);",
              "    ShipSpecialBonuses(ship,bonSlotArt,4);",
              "    str name = fem_name[Rnd(1,ArrayDim(fem_name)-1)];",
              "    SetName(ship,name);",
              "    ShipType(ship,'FemRanger');",
              "    NoScanToShip(ship,1);",
              "    NoTalkToShip(ship,1);",
              "    ShipFace(ship,Rnd(1,6));",
              "    ShipSkill(ship,0,4);",
              "    ShipSkill(ship,1,4);",
              "    ShipSkill(ship,2,Rnd(3,6));",
              "    for(i=0;i<5;i = i +1) {",
              "        ShipRefit(ship);",
              "    }",
              "    ShipJoin(Agent,ship);",
              "}",
              "function SpawnPirate(dword planet) {",
              "    planetRace = PlanetRace(planet);",
              "    PlanetRace(planet,Rnd(0,4));",
              "    dword ship = BuyPirate(planet,2000);",
              "    ShipJoinsClan(ship);",
              "    ShipType(ship,'Diversion');",
              "    PlanetRace(planet,planetRace);",
              "    hullstat=ShipItems(ship,0);",
              "    if(skilldivp['BonHp']){",
              "        ItemSize(ShipItems(ship,0),Rnd(700,1300)+max(3,GalaxyTechLevel())*10);",
              "    } else {",
              "        ItemSize(ShipItems(ship,0),Rnd(400,800)+max(3,GalaxyTechLevel())*5);",
              "    }",
              "    if(skilldivp['PremiumHull']){",
              "        int rndb = Rnd(1,3);",
              "        int hullbonus = FindBonusByCustomTag('Diversant'+rndb);",
              "        SpecialToEquipment(hullbonus, hullstat);",
              "    }",
              "    Chameleon(ship);",
              "    HullHP(ship,ItemSize(ShipItems(ship,0)));",
              "    if(GetEquipmentStats(hullstat,5)<1)ShipSpecialBonuses(ship,bonSlotDroid,1);",
              "    if(GetEquipmentStats(hullstat,7)<1)ShipSpecialBonuses(ship,bonSlotDef,1);",
              "    ShipSpecialBonuses(ship,bonSlotWeapon,5);",
              "    ShipSpecialBonuses(ship,bonSlotArt,4);",
              "    if(skilldivp['BufAttack']){",
              "        ShipSpecialBonuses(ship,bonWEnergy,10);",
              "        ShipSpecialBonuses(ship,bonWSplinter,10);",
              "        ShipSpecialBonuses(ship,bonWMissile,10);",
              "        ShipSpecialBonuses(ship,bonWRadius,100);",
              "        dword equip;",
              "        int rnd;",
              "        //Оснащаем артами",
              "        if(Rnd(1,5)>3){",
              "            rnd = Rnd(1,3);",
              "            if(rnd == 1){",
              "                equip=CreateArt(t_ArtPDTurret,ShipOwner(ship));",
              "            }",
              "            else if(rnd == 2){",
              "                equip=CreateArt(t_ArtEnergyPulse,ShipOwner(ship));",
              "            }",
              "            else {",
              "                equip=CreateArt(t_ArtSplinter,ShipOwner(ship));",
              "            }",
              "        }",
              "        ItemOwner(equip,7);",
              "        AddItemToShip(ship,equip);",
              "        ItemIsInUse(equip,ship,1);",
              "    }",
              "    if(ShipInPirateClan(Player())){",
              "        if(GetShipPirateRank(Player())>=3){",
              "            RelationToRanger(ship,Player(),Rnd(35,90));",
              "        }",
              "        else {",
              "            RelationToRanger(ship,Player(),Rnd(25,70));",
              "        }",
              "    }",
              "    else {",
              "        RelationToRanger(ship,Player(),Rnd(1,15));",
              "    }",
              "    ShipSkill(ship,0,4);",
              "    ShipSkill(ship,1,4);",
              "    ShipSkill(ship,2,Rnd(3,6));",
              "    if(skilldivp['GlobalBuf']){",
              "        for(i=0;i<6;i = i +1) {",
              "            ShipRefit(ship);",
              "        }",
              "    }",
              "    else {",
              "        for(i=0;i<4;i = i +1) {",
              "            ShipRefit(ship);",
              "        }",
              "    }",
              "    if(skilldivp['Def']){",
              "        ShipSpecialBonuses(ship,bonDef,5);",
              "    }",
              "    if(diversion==0){",
              "        TransferShip(ship,GroupShip(PB,0));",
              "    }",
              "",
              "",
              "    ShipJoin(Pirates,ship);",
              "}",
              "function ClearTstr() {",
              "    tstr1='';",
              "    tstr2='';",
              "    tstr3='';",
              "    tstr4='';",
              "    ArrayClear(skillprl);",
              "}",
              "function RandomPBGalaxy() {",
              "    SFT('REFLECTION-SP-AGENT:PB GALAXY-1');",
              "    dword CurSystem;",
              "    ArrayClear(systems);",
              "    for(i=0; i<GalaxyStars(); i=i+1) {",
              "      curSystem=GalaxyStar(i);",
              "      //Проверяем не пиратская ли система",
              "        if(StarOwner(curSystem) != 2) {",
              "            if(!StarBattle(curSystem)){",
              "                if(StarRuins(curSystem,'PB')) {",
              "                    //Записываем в массив систему",
              "                    ArrayAdd(systems, Id(curSystem));",
              "                }",
              "            }",
              "        }",
              "    }",
              "    SFT('REFLECTION-SP-AGENT:PB GALAXY-2');",
              "    tint = ArrayDim(systems);",
              "    SFT('REFLECTION-SP-AGENT:COUNT '+tint);",
              "    //Во избежание возможных ошибок после очисток массивов обращаюсь к массиву систем от первого индекса массива",
              "    if(tint >1){",
              "        system = systems[Rnd(1,tint-1)];",
              "    }",
              "    else {",
              "        system = 0;",
              "    }",
              "    SFT('REFLECTION-SP-AGENT:COUNT '+tint);",
              "    SFT('REFLECTION-SP-AGENT:PB DIVERSION SYSTEM: '+ Name(GalaxyStar(system-1)));",
              "    ArrayClear(systems);",
              "}",
              "function RandomGalaxy(int domikstat) {",
              "    SFT('REFLECTION-SP-AGENT:GALAXY-1');",
              "    dword CurSystem;",
              "    ArrayClear(systems);",
              "    for(i=0; i<GalaxyStars(); i=i+1) {",
              "      curSystem=GalaxyStar(i);",
              "      //Проверяем не пиратская ли система",
              "      if(StarOwner(curSystem) != 2) {",
              "          if(skilldivp['DomDiv'] == 1 && domikstat == 1){",
              "              if(!StarBattle(curSystem)){",
              "                  //Записываем в массив систему",
              "                  ArrayAdd(systems, Id(curSystem));",
              "              }",
              "          } else {",
              "              if(StarOwner(curSystem) == 0) {",
              "                  ArrayAdd(systems, Id(curSystem));",
              "              }",
              "          }",
              "",
              "      }",
              "    }",
              "    SFT('REFLECTION-SP-AGENT:GALAXY-2');",
              "    tint = ArrayDim(systems);",
              "    SFT('REFLECTION-SP-AGENT:COUNT '+tint);",
              "    //Во избежание возможных ошибок после очисток массивов обращаюсь к массиву систем от первого индекса массива",
              "    if(tint >1){",
              "        system = systems[Rnd(1,tint-1)];",
              "    }",
              "    else {",
              "        system = 0;",
              "    }",
              "    SFT('REFLECTION-SP-AGENT:COUNT '+tint);",
              "    SFT('REFLECTION-SP-AGENT:DIVERSION SYSTEM: '+ Name(GalaxyStar(system-1)));",
              "",
              "    ArrayClear(systems);",
              "}",
              "function StartDiversionNoPB(){",
              "    if(statebatt == 0){",
              "        RandomGalaxy(0);",
              "        if(system!=0){",
              "            statebatt = 1;",
              "            SFT('REFLECTION-SP-AGENT:DIV-LOG-TYPE-2');",
              "            planet = Id(FindPlanet(GalaxyStar(system-1),'NotNone',0,100));",
              "            if(Rnd(1,10) > 3 + tint3){",
              "                diversion = 1;",
              "                for(j=0;j < 2+GalaxyTechLevel()+Rnd(1,3)+skilldivp['BonMinusSwat']+skilldivp['GlobalBuf'];j = j + 1){",
              "                    SpawnPirate(IdToPlanet(planet));",
              "                }",
              "                for(j=0;j < 1+GalaxyTechLevel();j = j + 1){",
              "                    SpawnWarrior(IdToPlanet(planet));",
              "                }",
              "            }",
              "            else {",
              "                statebatt==0;",
              "            }",
              "        }",
              "        else {",
              "            statebatt = 0;",
              "        }",
              "    }",
              "}",
              "function StartDiversionPB(){",
              "    if(statebatt == 0){",
              "        RandomPBGalaxy();",
              "        if(system!=0){",
              "            statebatt = 1;",
              "            planet = Id(FindPlanet(GalaxyStar(system-1),'NotNone',0,100));",
              "            //Рандомайзер который определит сможет ли Коалиция заподозрить подготовку диверсии",
              "",
              "            if(Rnd(1,10) > 3 + tint3){",
              "                diversion = 2;",
              "                SFT('REFLECTION-SP-AGENT:DIV-LOG-TYPE-3-PB-FUNCTION');",
              "                for(j=0;j < 2+GalaxyTechLevel()+Rnd(1,3)+skilldivp['BonMinusSwat']+skilldivp['GlobalBuf'];j = j + 1){",
              "                    SpawnPirate(IdToPlanet(planet));",
              "                }",
              "            }",
              "            else {",
              "                SFT('REFLECTION-SP-AGENT:DIV-LOG-TYPE-1-PB-FUNCTION-PB-AGENT-DIV');",
              "                ShipJoin(PB,StarRuins(GalaxyStar(system-1),'PB'));",
              "                diversion=0;",
              "                SpawnAgent(IdToPlanet(planet));",
              "                turnr= CurTurn()+30;",
              "            }",
              "        }",
              "        else {",
              "            statebatt = 0;",
              "        }",
              "    }",
              "}",
              "function StartDisversion (){",
              "    if(statebatt == 0){",
              "        RandomGalaxy(1);",
              "        if(system!=0){",
              "            statebatt = 1;",
              "            diversion = 2;",
              "            SFT('REFLECTION-SP-AGENT:DIV-LOG-TYPE-3');",
              "            planet = Id(FindPlanet(GalaxyStar(system-1),'NotNone',0,100));",
              "",
              "            for(j=0;j < 2+GalaxyTechLevel()+Rnd(1,3)+skilldivp['BonMinusSwat']+skilldivp['GlobalBuf'];j = j + 1){",
              "                SpawnPirate(IdToPlanet(planet));",
              "            }",
              "        }",
              "        else {",
              "            statebatt = 0;",
              "        }",
              "    }",
              "}",
              "function StartCustomDisversion (){",
              "    if(statebatt == 0){",
              "        if(CurTurn()>=currucn){",
              "            if(sys){",
              "                SFT('REFLECTION-SP-AGENT:CUSTOM-DIV-LOG');",
              "                if(StarOwner(GalaxyStar(sys-1))!= 2){",
              "                    CustomEther('Div', 0, '',Format(CT('Script.RefSpAgentDefence.TextMsg.Diversion'),'<systemname'+'>',Name(GalaxyStar(sys-1))));",
              "                    planet = Id(FindPlanet(GalaxyStar(sys-1),'NotNone',0,100));",
              "                    statebatt=1;",
              "                    if(Rnd(1,10)>5){",
              "                        SFT('REFLECTION-SP-AGENT:CUSTOM-DIV-TYPE5');",
              "                        diversion = 4;",
              "                        for(j=0;j < shipcou;j = j + 1){",
              "                            SpawnPirate(IdToPlanet(planet));",
              "                        }",
              "                    }",
              "                    else {",
              "                        if(StarOwner(GalaxyStar(sys-1))!= 1){",
              "                            SFT('REFLECTION-SP-AGENT:CUSTOM-DIV-TYPE4');",
              "                            diversion = 3;",
              "                            for(j=0;j < shipcou;j = j + 1){",
              "                                SpawnPirate(IdToPlanet(planet));",
              "                            }",
              "                            for(j=0;j < 1+GalaxyTechLevel();j = j + 1){",
              "                                SpawnWarrior(IdToPlanet(planet));",
              "                            }",
              "                        }",
              "                        else {",
